{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "zerank-mcp",
	"main": "worker.js",
	"compatibility_date": "2025-03-10",
	"compatibility_flags": ["nodejs_compat"],
	"migrations": [
		{
			"new_sqlite_classes": ["MCPServer"],
			"tag": "v1"
		}
	],
	"durable_objects": {
		"bindings": [
			{
				"class_name": "MCPServer",
				"name": "MCP_SERVER"
			}
		]
	},
	"observability": {
		"enabled": true
	}
}