{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "zeroentropy-zerank-mcp",
	"main": "rerank.py",
	"compatibility_date": "2025-03-10",
	"compatibility_flags": ["nodejs_compat", "python_workers"],
	"migrations": [
		{
			"new_sqlite_classes": ["MyMCP"],
			"tag": "v1"
		}
	],
	"durable_objects": {
		"bindings": [
			{
				"class_name": "MyMCP",
				"name": "MCP_OBJECT"
			}
		]
	},
	"observability": {
		"enabled": true
	}
}